reset(arg) {  if (arg) return; set_light(1); } short() { if (set_light(0)) return            "A vast desolate desert"; return "dark room"; } init() { add_action("move1", "north"); add_action("move2", "south"); add_action("move3", "east"); add_action("move4", "west"); ex_init(); } move1() {  call_other(this_player(), "move_player", "north" + "#" + "players/ringer/pyramid/desert7"); return 1; } move2() {  call_other(this_player(), "move_player", "south" + "#" +            "players/ringer/d_edge"); return 1; } move3() {  call_other(this_player(), "move_player", "east" + "#" +            "players/ringer/pyramid/desert1"); return 1; } move4() {  call_other(this_player(), "move_player", "west" + "#" +            "players/ringer/pyramid/desert8"); return 1; } long(str) { if (set_light(0) == 0){ write("It is dark.\n"); return; }  write( "A desert stretches far into the distance to the north, east and west.  To\n"+ "south the desert seems to end.\n"); write("There are four obvious exits, " + "north" + ", " + "south" + ", " + "east" + " and " + "west" + ".\n"); }
