reset(arg) { if (!matilda || !living(matilda)) { object poker; matilda = clone_object("obj/monster"); call_other(matilda, "set_name", "matilda"); call_other(matilda, "set_level",9); call_other(matilda, "set_al",100); call_other(matilda, "set_short", "Matilda the fat old cook is here"); call_other(matilda, "set_aggressive", 1); call_other(matilda, "set_wc",10); call_other(matilda, "add_money", random(250)); move_object(matilda, this_object()); poker = clone_object("obj/weapon"); call_other(poker, "set_value",10); call_other(poker, "set_name", "poker"); call_other(poker, "set_wc",14); call_other(poker, "wield", "poker"); move_object(poker, matilda); } if (arg) return; set_light(1); } short() { if (set_light(0)) return           "Kitchen"; return "dark room"; } init() { add_action("move1", "north"); add_action("move2", "south");  } move1() {  call_other(this_player(), "move_player", "north" + "#" + "players/kantele/dining_room"); return 1; } move2() {  call_other(this_player(), "move_player", "south" + "#" +           "players/kantele/store_room"); return 1; } long(str) { if (set_light(0) == 0){ write("It is dark.\n"); return; }  write(          "A hot very hot kitchen, it looks very clean, you can see a store\n" +          "room to the south, to the north you can see the dining room.\n"); write("There are two obvious exits, " + "north" + " and " + "south" + ".\n"); }
