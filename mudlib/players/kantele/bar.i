reset(arg) {  if (arg) return; set_light(1); } short() { if (set_light(0)) return           "A small private barroom"; return "dark room"; } init() { add_action("move1", "north"); add_action("move2", "south"); add_action("move3", "west");  } move1() {  call_other(this_player(), "move_player", "north" + "#" + "players/kantele/living_room"); return 1; } move2() {  call_other(this_player(), "move_player", "south" + "#" +           "players/kantele/trophy_room"); return 1; } move3() {  call_other(this_player(), "move_player", "west" + "#" +           "players/kantele/behind_bar"); return 1; } long(str) { if (set_light(0) == 0){ write("It is dark.\n"); return; }  write(          "You are in a small private bar, you can exit west behind the bar\n" +          "or you can enter the trophy room to the south, to the north you\n" +          "can see into the living room.\n"); write("There are three obvious exits, " + "north" + ", " + "south" + " and " + "west" + ".\n"); }
