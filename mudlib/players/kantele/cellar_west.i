reset(arg) {  if (arg) return; set_light(0); } short() { if (set_light(0)) return           "You are in the west hall"; return "dark room"; } init() { add_action("move1", "east"); add_action("move2", "west"); add_action("move3", "north");  } move1() {  call_other(this_player(), "move_player", "east" + "#" + "players/kantele/stairway_down.c"); return 1; } move2() {  call_other(this_player(), "move_player", "west" + "#" +           "players/kantele/Kitiaras_anteroom.c"); return 1; } move3() {  call_other(this_player(), "move_player", "north" + "#" +           "players/kantele/dirty_passage.c"); return 1; } long(str) { if (set_light(0) == 0){ write("It is dark.\n"); return; }  write(          "You are in the west hall in the cellar, you can go east to the\n" +          "stairway, or follow passages north or west.\n"); write("There are three obvious exits, " + "east" + ", " + "west" + " and " + "north" + ".\n"); }
