reset(arg) {  if (arg) return; set_light(1); } short() { if (set_light(0)) return           "Dining room"; return "dark room"; } init() { add_action("move1", "east"); add_action("move2", "south");  } move1() {  call_other(this_player(), "move_player", "east" + "#" + "players/kantele/inner_hall"); return 1; } move2() {  call_other(this_player(), "move_player", "south" + "#" +           "players/kantele/kitchen"); return 1; } long(str) { if (set_light(0) == 0){ write("It is dark.\n"); return; }  write(          "A beautiful dining room, this room is decorated with dark cedar\n" +          "panels, you can smell coking to the south.\n"); write("There are two obvious exits, " + "east" + " and " + "south" + ".\n"); }
