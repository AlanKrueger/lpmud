reset(arg) {  if (arg) return; set_light(0); } short() { if (set_light(0)) return           "You are in a dirty passage"; return "dark room"; } init() { add_action("move1", "south"); add_action("move2", "down"); add_action("move3", "north");  } move1() {  call_other(this_player(), "move_player", "south" + "#" + "players/kantele/dirty_passage"); return 1; } move2() {  call_other(this_player(), "move_player", "down" + "#" +             "players/kantele/dirty_stairway"); return 1; } move3() {  call_other(this_player(), "move_player", "north" + "#" +             "players/kantele/dirty_room"); return 1; } long(str) { if (set_light(0) == 0){ write("It is dark.\n"); return; }  write(          "You are in a dirty passage, a stairway leads down and the \n" +          "corridor continues to the north.\n"); write("There are three obvious exits, " + "south" + ", " + "down" + " and " + "north" + ".\n"); }
